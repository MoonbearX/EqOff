@model EqOff.Models.Equipment

@{
    ViewData["Title"] = "Details";
}

<h1>Просмотр оборудования</h1>

<div>
    <h4>Equipment</h4>
    <hr />
    <dl class="row">
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.EqName)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.EqName)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Type)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Type)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.DateIn)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.DateIn)
        </dd>
    </dl>
</div>
<div>
    <a asp-action="Edit" asp-route-id="@Model?.EquipmentId">Редактировать оборудования</a> |
    <a asp-action="Index">Вернуться к списку оборудования</a>
</div>
